{
  "extends": ["react-app"],
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "camelcase": "off",
    "max-len": ["warn", {"code": 120}],
    "indent": [2, 2],
    "no-unused-vars": "error",
    "no-console": "warn",
    "jsx-quotes": ["error", "prefer-double"],
    "comma-dangle": ["error", "always-multiline"]
  }
}